{
  "name": "ai-dropshipper",
  "version": "1.2.0",
  "description": "AI-powered automatic dropshipping importer using RapidAPI, OpenAI, and Shopify, with a small web dashboard and Prisma analytics logging.",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "import": "node cli.js",
    "sync": "node cli.js",
    "dev": "NODE_ENV=development node src/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "prisma migrate dev --name init",
    "prisma:migrate-deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "6.4.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.19.0",
    "nodemailer": "^6.9.14",
    "openai": "^4.0.0",
    "p-limit": "^4.0.0"
  },
  "engines": {
    "node": "25.1.0"
  },
  "devDependencies": {
    "prisma": "^6.0.0"
  },
  "scripts": {
    "start": "prisma generate && node src/server.js"
  }
}

